    "def hanoi(n, A, B, C):\n",
    "    if n == 1:\n",
    "        print(A + '-->' + B)\n",
    "    else:\n",
    "        hanoi(n-1, A, C, B)\n",
    "        print(A + '-->' + B)\n",
    "        hanoi(n-1, C, A, B)\n",
    "        \n",
    "hanoi(4,'A','B','C')"
   ]
